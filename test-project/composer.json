{
    "name": "php-tuf/example-composer-project",
    "description": "Demo of a secured root composer.json",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "http://localhost:8080",
            "tuf": {
                "root": "root.json"
            }
        },
        {
            "type": "path",
            "url": "../"
        },
        {
            "type": "vcs",
            "url": "https://github.com/php-tuf/php-tuf.git"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "secure-http": false
    },
    "prefer-stable": true,
    "require": {
        "drupal/token": "^1.9"
    }
}
